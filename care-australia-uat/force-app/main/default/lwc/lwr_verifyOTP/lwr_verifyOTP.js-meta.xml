            <property name="logoutURL" label="Logout URL" type="String" required="true"/>
            <property name="resendOTPLabel" label="Resend OTP Button Label" type="String" required="true"/>
            <property name="otpLength" label="Length Of OTP" type="Integer" default="6" required="true"/>
