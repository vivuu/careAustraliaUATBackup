    <matchingRules>
        <fullName>Custom_Matching_Rule_for_Organisation_Merge</fullName>
        <booleanFilter>(1 AND 2) OR 3 OR (1 AND 4 AND 5 AND 6 AND 7 AND 8 AND 9)</booleanFilter>
        <description>Matching Rule for Organisation Account Merge</description>
        <label>Custom Matching Rule for Organisation Merge</label>
        <matchingRuleItems>
            <blankValueBehavior>NullNotAllowed</blankValueBehavior>
            <fieldName>Name</fieldName>
            <matchingMethod>Exact</matchingMethod>
        </matchingRuleItems>
        <matchingRuleItems>
            <blankValueBehavior>NullNotAllowed</blankValueBehavior>
