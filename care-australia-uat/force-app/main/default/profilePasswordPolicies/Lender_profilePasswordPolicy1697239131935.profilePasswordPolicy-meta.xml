<?xml version="1.0" encoding="UTF-8"?>
<ProfilePasswordPolicy xmlns="http://soap.sforce.com/2006/04/metadata">
    <forgotPasswordRedirect>true</forgotPasswordRedirect>
    <lockoutInterval>15</lockoutInterval>
    <maxLoginAttempts>3</maxLoginAttempts>
    <minimumPasswordLength>12</minimumPasswordLength>
    <minimumPasswordLifetime>true</minimumPasswordLifetime>
    <obscure>false</obscure>
    <passwordComplexity>5</passwordComplexity>
    <passwordExpiration>365</passwordExpiration>
    <passwordHistory>3</passwordHistory>
    <passwordQuestion>1</passwordQuestion>
    <profile>Lender</profile>
</ProfilePasswordPolicy>
